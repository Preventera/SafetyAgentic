    "agent_pipeline": {
        "total_target": 104,
        "current_developed": 25,
        "generation_enabled": true,
        "auto_deployment": false,
        "validation_required": true,
        "template_base": "agent_a2_configurable.py",
        "performance_threshold": 85.0,
        "timeout_seconds": 30,
        "retry_attempts": 3
    },
    "benchmarking": {
        "enabled": true,
        "real_time_updates": true,
        "comparison_metrics": [
            "average_absence_days",
            "injury_frequency",
            "severity_index",
            "fatality_rate",
            "lost_time_rate"
        ],
        "performance_thresholds": {
            "excellent": 20,
            "good": 10,
            "average": 0,
            "below_average": -10,
            "critical": -25
        },
        "update_frequency": "daily",
        "cache_benchmarks": true
    },
    "recommendations": {
        "evidence_based": true,
        "min_confidence": 0.8,
        "max_recommendations": 5,
        "include_roi_estimation": true,
        "include_implementation_steps": true,
        "priority_weighting": {
            "CRITIQUE": 3,
            "HAUTE": 2,
            "MOYENNE": 1
        },
        "sources_required": [
            "CNESST",
            "scientific_literature",
            "best_practices"
        ],
        "personalization_enabled": true
    },
    "data_sources": {
        "cnesst_csv_pattern": "data/lesions-{year}.csv",
        "cnesst_years_available": [
            2017,
            2018,
            2019,
            2020,
            2021,
            2022,
            2023
        ],
        "behavioral_db": "data/safetyagentic_behaviorx.db",
        "analytics_dbs": {
            "predictions": "analytics_predictions.db",
            "patterns": "analytics_patterns.db",
            "anomalies": "analytics_anomalies.db"
        },
        "cache_db": "data/cnesst_cache.db",
        "unified_state": "data/safetygraph_unified_state.db",
        "backup_frequency": "daily",
        "compression_enabled": true
    },
    "ui_components": {
        "app_behaviorx_enhancements": {
            "show_sector_detection": true,
            "show_benchmarks": true,
            "show_risk_comparison": true,
            "show_evidence_recommendations": true,
            "show_performance_vs_sector": true,
            "mobile_optimized": true
        },
        "cnesst_dashboard": {
            "enabled": true,
            "port": 8502,
            "tabs_enabled": [
                "vue_ensemble",
                "agents_mgmt",
                "scian_analytics",
                "perf_pipeline",
                "data_sources",
                "configuration"
            ],
            "real_time_updates": true,
            "auto_refresh_seconds": 30
        },
        "integration": {
            "cross_module_communication": true,
            "shared_state_enabled": true,
            "notification_system": true
        }
    },
    "performance": {
        "target_response_time": 3.0,
        "max_concurrent_analyses": 50,
        "cache_optimization": true,
        "lazy_loading": true,
        "compression": {
            "enabled": true,
            "algorithm": "gzip",
            "level": 6
        },
        "monitoring": {
            "enabled": true,
            "metrics_collection": true,
            "performance_logging": true,
            "alert_thresholds": {
                "response_time": 5.0,
                "error_rate": 0.05,
                "memory_usage": 0.8
            }
        }
    },
    "security": {
        "data_encryption": false,
        "access_control": false,
        "audit_logging": true,
        "data_retention_days": 365,
        "anonymization": {
            "enabled": false,
            "fields_to_anonymize": []
        }
    },
    "features": {
        "sector_auto_detection": {
            "enabled": true,
            "algorithms": [
                "keyword_matching",
                "nlp_classification"
            ],
            "confidence_display": true
        },
        "predictive_analytics": {
            "enabled": true,
            "models": [
                "random_forest",
                "xgboost",
                "lstm"
            ],
            "forecast_horizons": [
                3,
                6,
                12,
                24
            ]
        },
        "anomaly_detection": {
            "enabled": true,
            "algorithms": [
                "isolation_forest",
                "one_class_svm",
                "lstm_autoencoder"
            ],
            "sensitivity": "medium"
        },
        "pattern_recognition": {
            "enabled": true,
            "clustering_enabled": true,
            "trend_analysis": true,
            "correlation_analysis": true
        },
        "storm_research": {
            "enabled": true,
            "topics_count": 100,
            "auto_citation": true,
            "knowledge_update": "weekly"
        }
    },
    "deployment": {
        "environment": "development",
        "debug_mode": true,
        "logging_level": "INFO",
        "ports": {
            "main_app": 8501,
            "cnesst_dashboard": 8502
        },
        "startup_commands": [
            "streamlit run app_behaviorx.py --server.port 8501",
            "streamlit run app_cnesst_dashboard.py --server.port 8502"
        ],
        "health_checks": {
            "enabled": true,
            "endpoints": [
                "/health",
                "/status"
            ],
            "interval_seconds": 60
        }
    },
    "experimental": {
        "ai_powered_scian_detection": {
            "enabled": false,
            "model": "claude-3",
            "confidence_threshold": 0.9
        },
        "real_time_cnesst_sync": {
            "enabled": false,
            "api_endpoint": "https://api.cnesst.gouv.qc.ca",
            "sync_frequency": "hourly"
        },
        "multi_language_support": {
            "enabled": false,
            "languages": [
                "fr",
                "en"
            ],
            "auto_translation": false
        },
        "advanced_visualizations": {
            "enabled": true,
            "3d_charts": false,
            "interactive_heatmaps": true,
            "geographical_mapping": false
        }
    },
    "maintenance": {
        "auto_cleanup": {
            "enabled": true,
            "cache_cleanup_days": 7,
            "log_cleanup_days": 30,
            "temp_files_cleanup": true
        },
        "backup": {
            "enabled": true,
            "frequency": "daily",
            "retention_days": 30,
            "compression": true,
            "location": "backups/"
        },
        "updates": {
            "auto_check": true,
            "notification": true,
            "staging_environment": true
        }
    },
    "metadata": {
        "version": "2.0.0",
        "created_date": "2024-07-11",
        "last_modified": "2024-07-11T14:30:00Z",
        "author": "Mario Genest - GenAISafety",
        "description": "Configuration complète intégration CNESST dans SafetyGraph",
        "compatibility": {
            "python_version": ">=3.8",
            "streamlit_version": ">=1.28.0",
            "required_packages": [
                "pandas>=1.5.0",
                "plotly>=5.0.0",
                "sqlite3",
                "json",
                "datetime",
                "typing",
                "logging",
                "pathlib"
            ]
        }
    }
